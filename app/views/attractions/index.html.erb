<% @attractions.each do |attraction| %>
<% if User.find(session[:user_id]).admin %>
<%= link_to "Show #{attraction.name}", attraction_path(attraction) %> <br><br>
<% else %>
<%= link_to "Go on #{attraction.name}", attraction_path(attraction) %><br><br>
<% end %>
<% end %>
<% if User.find(session[:user_id]).admin %>
<%= link_to "New Attraction", new_attraction_path %>
<% end %>
