<%= @attraction.name %>
<% if User.find(session[:user_id]).admin %>
<%= link_to "Edit Attraction", edit_attraction_path %>
<% end %>
<%= @attraction.nausea_rating %>
<%= @attraction.tickets %>
<%= @attraction.happiness_rating %>
<%= @attraction.min_height %>
<%= form_for (@ride) do |t| %>
<%= t.hidden_field :attraction_id, {value: @attraction.id} %>
<%= t.submit "Go on this ride"  %>
<% end %>
